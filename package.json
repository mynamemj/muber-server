  "description": "Server for the (M)Uber Clone on GraphQL, Typescript, NodeJS",
  "main": "index.js",
  "repository": "https://github.com/mynamemj/muber-server",
  "author": "mynamemj <dlaudwp1313@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.6",
    "babel-runtime": "^6.26.0",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^1.18.11",
    "ts-node": "^8.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  },
  "scripts": {
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts,graphql",
